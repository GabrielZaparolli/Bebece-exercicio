            "blockClass": "close-menu-mobile"
        }
    },
    "info-card#banner-menu-header": {
        "title": "Banner Menu Header",
        "props": {
            "imageUrl": "assets/images/bannerPrincipal1.jpg",
            "headline": "Celebration - 20 anos",
            "callToActionText": "Conhe√ßa",
            "callToActionUrl": "/",
            "isFullModeStyle": true,
