{
    "drawer#menu-mobile": {
        "title": "Menu Mobile",
        "props": {
            "blockClass": "container-menu-mobile"
        },
        "children": [
            "flex-layout.col#menu-mobile"
        ]
    },
    "flex-layout.col#menu-mobile": {
        "props": {
            "blockClass": "menu-mobile"
        },
        "children": [
            "flex-layout.row#menu-mobile-header",
            "info-card#banner-menu-header"
            // "flex-layout.col#list-menu-mobile"
        ]
    },
    "flex-layout.row#menu-mobile-header": {
        "title": "Menu Mobile Header",
        "props": {
            "blockClass": "menu-mobile-header",
            "horizontalAlign": "between",
            "verticalAlign": "middle"
        },
        "children": [
            "flex-layout.row#logo-menu-mobile",
            "flex-layout.row#close-menu-mobile"
        ]
    },
    "flex-layout.row#logo-menu-mobile": {
        "children": [
            "logo#header"
