            "rich-text#banner-inst-text-2",
            "link#banner-inst-btn-2"
        ],
        "props": {
            "blockClass": "banner-inst-content",
            "verticalAlign": "bottom",
            "horizontalAlign": "left",
            "preventVerticalStretch": true
        }
    },
    "rich-text#banner-inst-title-2": {
        "title": "Titulo banner institucional 2",
        "props": {
            "text": "É AMANHÃ",
            "blockClass": "banner-inst-title"
        }
    },
    "rich-text#banner-inst-text-2": {
        "title": "Descrição banner institucional 2",
        "props": {
            "text": "SIMPLE and TRUE: lançamento da nova coleção Outono Inverno 2024 da Bebecê ❤️",
            "blockClass": "banner-inst-text"
        }
    },
    "link#banner-inst-btn-2": {
        "title": "Link banner institucional 2",
        "props": {
            "href": "/",
            "label": "Saiba mais!",
            "blockClass": "banner-inst-btn"
        }
    },
    // Banner 3
