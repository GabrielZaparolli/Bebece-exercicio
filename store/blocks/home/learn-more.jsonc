        "children": [
            "image#banner-inst-bg-3",
            "flex-layout.col#banner-inst-content-3"
        ],
        "props": {
            "blockClass": "container-banner-inst"
        }
    },
    "image#banner-inst-bg-3": {
        "title": "Imagem banner institucional 3",
        "props": {
            "src": "assets/images/bannerInstitucional/banner-institu3.jpg",
            "alt": "Banner institucional 3",
            "blockClass": "banner-inst-bg"
        }
    },
    "flex-layout.col#banner-inst-content-3": {
        "title": "Descrição banner institucional 3",
        "children": [
            "rich-text#banner-inst-title-3",
            "rich-text#banner-inst-text-3",
            "link#banner-inst-btn-3"
        ],
        "props": {
            "blockClass": "banner-inst-content",
            "verticalAlign": "bottom",
